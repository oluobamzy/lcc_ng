{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "storage": {
    "cors": [
      {
        "origin": ["http://localhost:5173"], 
        "method": ["GET", "POST", "PUT"],
        "maxAgeSeconds": 3600
      }
    ]
  }
  }
}
